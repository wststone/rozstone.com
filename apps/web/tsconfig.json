{
	"extends": "tsconfig/nextjs.json",
	"exclude": ["node_modules"],
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@root/*": ["*"],
			"@components/*": ["src/components/*"],
			"@types/*": ["src/types/*"],
			"@types": ["src/types/"],
			"@logics": ["src/logics/"],
			"@hooks": ["src/hooks/"],
			"@styles/*": ["./styles/*"],
			"@store": ["src/store/"],
		}
	}
}
